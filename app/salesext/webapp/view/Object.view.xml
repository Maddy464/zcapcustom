<mvc:View controllerName="salesext.controller.Object" xmlns="sap.uxap" xmlns:layout="sap.ui.layout"
	xmlns:m="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.f.semantic" xmlns:footerbar="sap.ushell.ui.footerbar">
	<semantic:SemanticPage id="page212" headerPinnable="false" toggleHeaderOnTitleClick="false" busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">
		<semantic:content>
			<ObjectPageLayout id="_IDGenObjectPageLayout1">
				<headerTitle>
					<ObjectPageHeader id="_IDGenObjectPageHeader" objectTitle="{CustomerName}" objectSubtitle="{Note}">
						<actions>
							<ObjectPageHeaderActionButton id="_IDGenObjectPageHeaderActionButton" icon="sap-icon://shipping-status" tooltip="{i18n>goodsIssueActionTooltip}" type="Emphasized"
								visible="{ parts: ['LifecycleStatus', 'DeliveryStatus'], formatter: '.formatter.goodsIssuePossible' }" press="onGoodsIssuePressed"/>
							<ObjectPageHeaderActionButton id="_IDGenObjectPageHeaderActionButton1" icon="sap-icon://receipt" tooltip="{i18n>invoiceActionTooltip}" type="Emphasized"
								visible="{ parts: ['LifecycleStatus', 'DeliveryStatus', 'BillingStatus'], formatter: '.formatter.invoicePossible' }" press="onInvoicePressed"/>
							<ObjectPageHeaderActionButton id="_IDGenObjectPageHeaderActionButton2" icon="sap-icon://accept" tooltip="{i18n>confirmActionTooltip}" type="Accept"
								visible="{ path: 'LifecycleStatus', formatter: '.formatter.confirmPossible' }" press="onConfirmPressed"/>
							<ObjectPageHeaderActionButton id="_IDGenObjectPageHeaderActionButton3" icon="sap-icon://decline" tooltip="{i18n>cancelActionTooltip}" type="Reject"
								visible="{ parts: ['LifecycleStatus', 'DeliveryStatus'], formatter: '.formatter.cancelPossible' }" press="onCancelPressed"/>
						</actions>
					</ObjectPageHeader>
				</headerTitle>
				<headerContent>
					<layout:VerticalLayout id="_IDGenVerticalLayout">
						<m:Label id="_IDGenLabel" text="{i18n>lifecycleStatusLabel}"/>
						<m:ObjectStatus id="_IDGenObjectStatus" text="{LifecycleStatusDescription}" state="{ path: 'LifecycleStatus', formatter: '.formatter.objectStatusValueState' }"
							icon="{ path: 'LifecycleStatus', formatter: '.formatter.objectStatusIcon' }"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout1">
						<m:Label id="_IDGenLabel1" text="{i18n>deliveryStatusLabel}"/>
						<m:ObjectStatus id="_IDGenObjectStatus1" text="{DeliveryStatusDescription}"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout2">
						<m:Label id="_IDGenLabel2" text="{i18n>billingStatusLabel}"/>
						<m:ObjectStatus id="_IDGenObjectStatus2" text="{BillingStatusDescription}"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout3">
						<m:Label id="_IDGenLabel3" text="{i18n>createdAtLabel}"/>
						<m:ObjectStatus id="_IDGenObjectStatus3" text="{ path: 'CreatedAt', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium' } }"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout4">
						<m:Label id="_IDGenLabel4" text="{i18n>changedAtLabel}"/>
						<m:ObjectStatus id="_IDGenObjectStatus4" text="{ path: 'ChangedAt', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium' } }"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout5">
						<m:Label id="_IDGenLabel5" text="{i18n>GrossAmountLabel}"/>
						<m:ObjectNumber id="_IDGenObjectNumber1" number="{ path: 'GrossAmount', formatter: '.formatter.numberUnit' }" unit="{CurrencyCode}"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout6">
						<m:Label id="_IDGenLabel6" text="{i18n>netAmountLabel}"/>
						<m:ObjectNumber id="_IDGenObjectNumber2" number="{ path: 'NetAmount', formatter: '.formatter.numberUnit' }" unit="{CurrencyCode}"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout id="_IDGenVerticalLayout7">
						<m:Label id="_IDGenLabel7" text="{i18n>taxAmountLabel}"/>
						<m:ObjectNumber id="_IDGenObjectNumber3" number="{ path: 'TaxAmount', formatter: '.formatter.numberUnit' }" unit="{CurrencyCode}"/>
					</layout:VerticalLayout>
				</headerContent>
				<sections>
					<ObjectPageSection id="_IDGenObjectPageSection" title="{i18n>itemsSectionTitle}">
						<subSections>
							<ObjectPageSubSection id="_IDGenObjectPageSubSection">
								<blocks>
									<m:Table id="_IDGenTable" items="{ToLineItems}" noDataText="{i18n>itemsTableNoDataText}" width="auto">
										<m:columns>
											<m:Column id="_IDGenColumn5" width="15%">
												<m:Text id="_IDGenText10" text="{i18n>tableItemIdColumnTitle}"/>
											</m:Column>
											<m:Column id="_IDGenColumn6">
												<m:Text id="_IDGenText11" text="{i18n>tableProductColumnTitle}"/>
											</m:Column>
											<m:Column id="_IDGenColumn7" width="15%">
												<m:Text id="_IDGenText12" text="{i18n>tableDeliveryDateColumnTitle}"/>
											</m:Column>
											<m:Column id="_IDGenColumn8" hAlign="End" width="20%">
												<m:Text id="_IDGenText13" text="{i18n>tableQuantityColumnTitle}"/>
											</m:Column>
											<m:Column id="_IDGenColumn9" hAlign="End" width="20%">
												<m:Text id="_IDGenText14" text="{i18n>tableGrossAmountColumnTitle}"/>
											</m:Column>
										</m:columns>
										<m:items>
											<m:ColumnListItem id="_IDGenColumnListItem1" type="Inactive">
												<m:cells>
													<m:ObjectIdentifier id="_IDGenObjectIdentifier1"
														title="{ path: 'ItemPosition', type: 'sap.ui.model.odata.type.String', constraints: { isDigitSequence: true, maxLength: 10 } }"/>
													<m:ObjectIdentifier id="_IDGenObjectIdentifier2" title="{ProductID}" text="{ToProduct/Name}"/>
													<m:Text id="_IDGenText15" text="{ path: 'DeliveryDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium' } }"/>
													<m:ObjectNumber id="_IDGenObjectNumber4" number="{ path: 'Quantity', formatter: '.formatter.numberUnit' }" unit="{QuantityUnit}"/>
													<m:ObjectNumber id="_IDGenObjectNumber5" number="{ path: 'GrossAmount', formatter: '.formatter.numberUnit' }" unit="{CurrencyCode}"/>
												</m:cells>
											</m:ColumnListItem>
										</m:items>
									</m:Table>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection>
				</sections>
			</ObjectPageLayout>
		</semantic:content>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:shareInJamAction>
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>
		</semantic:shareInJamAction>
		<semantic:saveAsTileAction>
			<footerbar:AddBookmarkButton id="shareTile" title="{objectView>/saveAsTileTitle}"/>
		</semantic:saveAsTileAction>
	</semantic:SemanticPage>
</mvc:View>